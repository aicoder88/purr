{
  "name": "purrify",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "optimize:watch": "node --experimental-modules scripts/auto-optimize.js",
    "optimize:once": "node --experimental-modules scripts/auto-optimize.js --run-once",
    "prebuild": "node scripts/vercel-prebuild.js",
    "build": "npx update-browserslist-db@latest && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "fix": "npm run lint -- --fix && npm run format",
    "test": "jest --config=jest.config.ts",
    "test:watch": "jest --watch --config=jest.config.ts",
    "test:coverage": "jest --coverage --config=jest.config.ts",
    "check-types": "node scripts/check-types.js",
    "postbuild": "next-sitemap && npm run generate-enhanced-sitemap",
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-all-images": "node scripts/optimize-all-images.js",
    "optimize-performance": "node scripts/optimize-performance.js",
    "add-image-dimensions": "node scripts/add-image-dimensions.js",
    "types:supabase": "echo '// Placeholder for Supabase types' > src/types/supabase.ts",
    "update-deps": "npx update-browserslist-db@latest",
    "generate-enhanced-sitemap": "node scripts/generate-sitemap.js",
    "predev": "node scripts/clear-webpack-cache.js",
    "analyze": "ANALYZE=true next build",
    "analyze-js": "node scripts/analyze-js.js",
    "find-unused-deps": "npx depcheck",
    "clear-cache": "node scripts/clear-webpack-cache.js",
    "purge-vercel-cache": "node scripts/purge-vercel-cache.js",
    "build:production": "node scripts/build-production.js",
    "seo:optimize": "node scripts/seo-optimization.js",
    "bundle:analyze": "node scripts/bundle-analysis.js",
    "cache:optimize": "node scripts/cache-optimization.js",
    "performance:audit": "npm run seo:optimize && npm run bundle:analyze && npm run cache:optimize"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^3.6.0",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@vercel/analytics": "^1.5.0",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "critters": "^0.0.23",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.1.5",
    "framer-motion": "^11.18.0",
    "googleapis": "^148.0.0",
    "lucide-react": "^0.394.0",
    "micro": "^10.0.1",
    "next-auth": "^4.24.11",
    "next-seo": "^6.6.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^2.0.19",
    "stripe": "^18.2.1",
