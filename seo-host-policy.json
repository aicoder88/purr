{
  "canonicalHost": "https://www.purrify.ca",
  "redirectHosts": [
    "https://purrify.ca",
    "http://purrify.ca",
    "http://www.purrify.ca"
  ],
  "allowedAnalyticsQueryParams": [
    "ref",
    "src",
    "ctx",
    "med"
  ],
  "canonicalizeParameterizedUrls": true,
  "parameterizedUrlRobots": "noindex, follow",
  "sitemap": {
    "indexPath": "/sitemap-index.xml",
    "members": [
      "/sitemap.xml"
    ]
  },
  "moneyPages": [
    "/products/",
    "/products/trial-size/",
    "/try-free/"
  ],
  "educationalHubs": [
    "/blog/",
    "/learn/",
    "/learn/faq/",
    "/learn/science/"
  ],
  "internalLinkRouting": [
    {
      "sourcePattern": "/learn/solutions/*",
      "targets": [
        {
          "url": "/products/trial-size/",
          "anchor": "free activated carbon cat litter deodorizer trial"
        },
        {
          "url": "/products/",
          "anchor": "activated carbon cat litter deodorizer options"
        },
        {
          "url": "/learn/faq/",
          "anchor": "cat litter deodorizer FAQ"
        },
        {
          "url": "/learn/science/",
          "anchor": "activated carbon litter science"
        }
      ]
    },
    {
      "sourcePattern": "/blog/*",
      "targets": [
        {
          "url": "/products/trial-size/",
          "anchor": "try Purrify granules on your litter box"
        },
        {
          "url": "/learn/",
          "anchor": "complete litter odor learning hub"
        }
      ]
    }
  ]
}
